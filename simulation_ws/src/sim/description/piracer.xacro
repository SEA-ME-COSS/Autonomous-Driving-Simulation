  <!-- LINK: base_footprint -->
  <link name="base_footprint"></link>

  <!-- LINK: chassis -->
  <link name="chassis">
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="1000.0"/>
      <origin xyz="0.118 0 0" rpy="0 0 0"/>
